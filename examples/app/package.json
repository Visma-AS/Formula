{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/formula",
  "scripts": {
    "prebuild": "npm run generate && npx recursive-copy-cli ../../dist public/formula-frontend --overwrite",
    "build": "craco build",
    "eject": "react-scripts eject",
    "generate": "npm-run-all --parallel generate:*",
    "generate:messages": "formatjs-scripts",
    "prestart": "npm run generate && npx recursive-copy-cli ../../dist public/formula-frontend --overwrite",
    "start": "craco start",
    "test": "craco test --passWithNoTests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@visma/craco-plugin-super-template": "^0.1.3",
    "@visma/formula": "^0.4.52",
    "@visma/react-app-super-template": "^0.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
