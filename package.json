{
  "name": "@visma/formula",
  "version": "0.4.46",
  "description": "React component for configurable forms. Optionally connect to the backend to fetch external config and submit form data to.",
  "homepage": "https://github.com/Visma-AS/formula#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Visma-AS/formula.git"
  },
  "license": "ISC",
  "author": {
    "name": "Arno Saine",
    "email": "arno.saine@visma.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/main.js"
  },
  "main": "./lib/main.cjs",
  "module": "./lib/main.js",
  "files": [
    "lang",
    "lib",
    "api.json"
  ],
  "scripts": {
    "generate-client": "react-openapi-client-generator api.json src/client.js",
    "prebuild": "npm run messages:extract && npm run messages:compile && npm run generate-client",
    "build": "library-scripts build",
    "prepare": "npm run build",
    "prewatch": "npm run generate-client",
    "watch": "library-scripts watch",
    "messages:extract": "formatjs extract 'src/**/*.{js,jsx,ts,tsx}' --out-file lang/fi-FI.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "messages:compile": "formatjs compile-folder --ast lang lib/compiled-lang"
  },
  "eslintConfig": {
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "dependencies": {
    "@visma/react-openapi-client-generator": "^0.1.3",
    "@visma/rjsf-core": "^3.1.0-9",
    "@visma/rjsf-material-ui": "^3.1.0-9",
    "axios": "^0.21.0",
    "base64-js": "^1.5.1",
    "date-fns": "^2.22.1",
    "immer": "^9.0.2",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.2",
    "moment": "^2.29.1",
    "pretty-bytes": "^5.6.0",
    "react-dates": "^21.8.0",
    "react-google-recaptcha": "^2.1.0",
    "react-rte": "^0.16.3",
    "sift": "^15.0.0",
    "tiny-invariant": "^1.1.0",
    "use-axios": "^0.3.7",
    "util-deprecate": "^1.0.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.29",
    "@postinumero/library-scripts": "^0.1.4",
    "babel-plugin-formatjs": "^10.2.15"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.12.0"
  },
  "publishConfig": {
    "tag": "latest"
  }
}
